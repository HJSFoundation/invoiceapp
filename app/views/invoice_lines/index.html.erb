<h1>Listing invoice_lines</h1>

<table>
  <tr>
    <th>Invoice header</th>
    <th>Biller</th>
    <th>Matter</th>
    <th>Rate</th>
    <th>Quantity</th>
    <th>Line total</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @invoice_lines.each do |invoice_line| %>
  <tr>
    <td><%= invoice_line.invoice_header_id %></td>
    <td><%= invoice_line.biller_id %></td>
    <td><%= invoice_line.matter_id %></td>
    <td><%= invoice_line.rate %></td>
    <td><%= invoice_line.quantity %></td>
    <td><%= invoice_line.line_total %></td>
    <td><%= link_to 'Show', invoice_line %></td>
    <td><%= link_to 'Edit', edit_invoice_line_path(invoice_line) %></td>
    <td><%= link_to 'Destroy', invoice_line, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Invoice line', new_invoice_line_path %>
